    "def read_unique_items_from_file(file: str) -> list:\n",
    "    if os.path.exists(file):\n",
    "        with open(file, \"r\", errors=\"ignore\") as f:\n",
    "            return list(set(url.strip() for url in f.readlines() if url.strip()))\n",
    "    return []\n",
    "\n",
    "def read_dict_from_csv(file: str, key: str = \"URL\", value: str = \"Title\") -> dict:\n",
    "    if os.path.exists(file):\n",
    "        return pd.read_csv(file, encoding_errors=\"ignore\").set_index(key)[value].to_dict()\n",
    "    return {}\n",
    "\n",
    "def ask_user(prompt: str) -> bool:\n",
    "    while True:\n",
    "        try: answer = input(prompt).strip().lower()\n",
    "        except: continue\n",
    "        if not answer: continue\n",
    "        if (\"yes\").startswith(answer.lower()): return True\n",
    "        elif (\"no\").startswith(answer.lower()): return False\n",
    "\n",
    "def display_video(url: str): display(IFrame(url, width=490, height=275, extras=[\"controls\", \"autoplay\", 'allow=\"autoplay\"']))\n",
    "\n",
    "def get_yt_id(url: str):\n",
    "    # Find the part after the \"?\" (query parameters)\n",
    "    if \"?\" in url: query_string = url.split(\"?\")[-1]\n",
    "    else: return None\n",
    "    # Split the query parameters by \"&\"\n",
    "    params = query_string.split(\"&\")\n",
    "    # Find the parameter by name\n",
    "    for param in params:\n",
    "        key, value = param.split(\"=\")\n",
    "        if key == \"v\": return value\n",
    "    return None\n",
    "\n",
    "def get_publish_year(relative_time):\n",
    "    try:\n",
    "        current_date = datetime.now()\n",
    "        # Check for \"years ago\"\n",
    "        if \"year\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            years_ago = int(match.group(1))\n",
    "            publish_date = current_date.replace(year=current_date.year - years_ago)\n",
    "        # Check for \"months ago\"\n",
    "        elif \"month\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            months_ago = int(match.group(1))\n",
    "            new_month = current_date.month - months_ago\n",
    "            new_year = current_date.year\n",
    "            if new_month <= 0:  # Handle year rollover\n",
    "                new_year -= (abs(new_month) // 12) + 1\n",
    "                new_month = (12 + new_month) % 12 or 12\n",
    "            publish_date = current_date.replace(year=new_year, month=new_month)\n",
    "        # Check for \"weeks ago\"\n",
    "        elif \"week\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            weeks_ago = int(match.group(1))\n",
    "            publish_date = current_date - timedelta(weeks=weeks_ago)\n",
    "        # Check for \"days ago\"\n",
    "        elif \"day\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            days_ago = int(match.group(1))\n",
    "            publish_date = current_date - timedelta(days=days_ago)\n",
    "        # Check for \"hours ago\"\n",
    "        elif \"hour\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            hours_ago = int(match.group(1))\n",
    "            publish_date = current_date - timedelta(hours=hours_ago)\n",
    "        # Check for \"minutes ago\"\n",
    "        elif \"minute\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            minutes_ago = int(match.group(1))\n",
    "            publish_date = current_date - timedelta(minutes=minutes_ago)\n",
    "        # Check for \"seconds ago\"\n",
    "        elif \"second\" in relative_time:\n",
    "            match = re.search(r'(\\d+)', relative_time)\n",
    "            if not match: return None\n",
    "            seconds_ago = int(match.group(1))\n",
    "            publish_date = current_date - timedelta(seconds=seconds_ago)\n",
    "        else: return None\n",
    "        return publish_date.year\n",
    "    except: return None"
   ]
