    "# Get Existing YouTube Ids\n",
    "used_yt_dict = {\n",
    "    get_yt_id(url): title\n",
    "    for url, title in read_dict_from_csv(os.path.join(output_path, used_urls_filename)).items()\n",
    "    if get_yt_id(url)\n",
    "}\n",
    "unused_yt_ids = set([\n",
    "    get_yt_id(url)\n",
    "    for url in read_unique_items_from_file(os.path.join(output_path, unused_urls_filename))\n",
    "    if get_yt_id(url)\n",
    "])\n",
    "# YouTube Search Query\n",
    "search_query = re.sub(r'\\s+', \" \", \"\"\"\n",
    "\n",
    "    (trump OR harris OR kamala OR donald)\n",
    "    (arizona OR A.Z.)\n",
    "    -campaign -rally -assassin -shot -live\n",
    "    \n",
    "\"\"\".replace(\"\\n\", \" \")).strip()\n",
    "# Filters for Title and Description\n",
    "words_that_should_be_in_title = {\n",
    "    \"1\": [\"kamala\", \"harris\", \"trump\", \"donald\"],\n",
    "    \"2\": [\"arizona\", \"A.Z.\"]\n",
    "}\n",
    "words_that_should_be_in_title_or_description = {\n",
    "    # \"2\": [\"arizona\", \"A.Z.\"]\n",
    "}\n",
    "words_that_should_not_be_in_title = [\n",
    "    # \"pennsylvania\", \"michigan\",\n",
    "    # \"donald\", \"trump\",\n",
    "    # \"kamala\", \"harris\",\n",
    "    # \"campaign\", \"campaigns\", \"campaigning\",\n",
    "    # \"rally\", \"rallies\", \"rallying\",\n",
    "    # \"shot\", \"shots\", \"shoot\", \"shoots\", \"shooting\"\n",
    "    # \"assassination\",\n",
    "    # \"live\"\n",
    "]\n",
    "words_that_should_not_be_in_title_and_description = [\n",
    "    # \"pennsylvania\", \"michigan\",\n",
    "    # \"donald\", \"trump\",\n",
    "    # \"kamala\", \"harris\",\n",
    "    # \"campaign\", \"campaigns\", \"campaigning\",\n",
    "    # \"rally\", \"rallies\", \"rallying\",\n",
    "    # \"shot\", \"shots\", \"shoot\", \"shoots\", \"shooting\"\n",
    "    # \"assassination\",\n",
    "    # \"live\"\n",
    "]\n",
    "# YouTube Filters\n",
    "yt_filters = \"EgQQARgD\" # Try Niyo Nalang Baguhin\n",
    "# Search YouTube Videos\n",
    "print(f'...Searching...')\n",
    "yt_list = CustomSearch(\n",
    "    search_query,\n",
    "    yt_filters\n",
    ")\n",
    "yt_videos = yt_list.result()[\"result\"]\n",
    "last_yt_videos_len = len(yt_videos)\n",
    "# Loop To Search Per Batch\n",
    "while True:\n",
    "    for yt in yt_videos:\n",
    "        url = yt[\"link\"].strip()\n",
    "        video_id = yt[\"id\"]\n",
    "        title = yt[\"title\"]\n",
    "        descriptionSnippet = None\n",
    "        try: descriptionSnippet = ''.join(item[\"text\"] for item in yt[\"descriptionSnippet\"]).replace(\"\\xa0\", \"\").strip()\n",
    "        except: None\n",
    "        # Check If Video / Url Already Seen\n",
    "        if (\n",
    "            not title\n",
    "            or not video_id\n",
    "            or video_id in used_yt_dict\n",
    "            or video_id in unused_yt_ids\n",
    "        ): continue\n",
    "        # Check If Year > 2020\n",
    "        upload_year = get_publish_year(yt[\"publishedTime\"]) if yt[\"publishedTime\"] else None\n",
    "        if upload_year is not None and upload_year <= 2020: continue\n",
    "        # Word Filters\n",
    "        should_skip = False\n",
    "        # Check Words Required in Title\n",
    "        for terms in words_that_should_be_in_title.values(): \n",
    "            if all(\n",
    "                f' {word.strip().lower()} ' not in f' {title.strip().lower()} '\n",
    "                for word in terms\n",
    "            ): \n",
    "                should_skip = True\n",
    "                break\n",
    "        if should_skip: continue\n",
    "        # Check Words Required in Title or Description\n",
    "        for terms in words_that_should_be_in_title_or_description.values(): \n",
    "            if all(\n",
    "                f' {word.strip().lower()} ' not in f' {title.strip().lower()} ' \n",
    "                and (\n",
    "                    not descriptionSnippet\n",
    "                    or f' {word.strip().lower()} ' not in f' {descriptionSnippet.strip().lower()} '\n",
    "                )\n",
    "                for word in terms\n",
    "            ): \n",
    "                should_skip = True\n",
    "                break\n",
    "        if should_skip: continue\n",
    "        # Check Words Prohibited in Title\n",
    "        if any(\n",
    "            f' {word.strip().lower()} ' in f' {title.strip().lower()} ' \n",
    "            for word in words_that_should_not_be_in_title\n",
    "        ): continue\n",
    "        # Check Words Prohibited in Title and Description\n",
    "        if any(\n",
    "            f' {word.strip().lower()} ' in f' {title.strip().lower()} ' \n",
    "            or (\n",
    "                descriptionSnippet\n",
    "                and f' {word.strip().lower()} ' in f' {descriptionSnippet.strip().lower()} '\n",
    "            )\n",
    "            for word in words_that_should_not_be_in_title_and_description\n",
    "        ): continue\n",
    "        # Reset Output Console\n",
    "        clear_output(wait=True)\n",
    "        # Display the Vid\n",
    "        display_video(f'https://www.youtube.com/embed/{video_id}?autoplay=1')\n",
    "        print(\"\") # Just New Line for Better Output\n",
    "        print(f'Title: {pcolors.BRIGHTCYAN}{title}{pcolors.ENDC}')\n",
    "        print(\"\") # Just New Line for Better Output\n",
    "        print(f'Description: {pcolors.BRIGHTCYAN}{descriptionSnippet if descriptionSnippet else \"None\"}{pcolors.ENDC}')\n",
    "        print(\"\") # Just New Line for Better Output\n",
    "        print(f'Publish Year: {pcolors.BRIGHTCYAN}{upload_year if upload_year is not None else \"None\"}{pcolors.ENDC}')\n",
    "        print(\"\") # Just New Line for Better Output\n",
    "        print(f'URL If Video Failed To Load: {url}')\n",
    "        print(\"\") # Just New Line for Better Output\n",
    "        if ask_user(\"Do you want to add this Video URL (y/n)?\"):\n",
    "            # Create Collection Directory\n",
    "            os.makedirs(output_path, exist_ok=True)\n",
    "            used_yt_dict[video_id] = title\n",
    "            (\n",
    "                pd.DataFrame(list(({\n",
    "                    f'https://www.youtube.com/watch?v={yt_id}': title\n",
    "                    for yt_id, title in used_yt_dict.items()\n",
    "                    if yt_id\n",
    "                }).items()), columns=[\"URL\", \"Title\"])\n",
    "                .to_csv(os.path.join(output_path, used_urls_filename), index=False, errors=\"ignore\")\n",
    "            )\n",
    "        else:\n",
    "            # Create Collection Directory\n",
    "            os.makedirs(output_path, exist_ok=True)\n",
    "            with open(os.path.join(output_path, unused_urls_filename), \"w\", errors=\"ignore\") as file:\n",
    "                unused_yt_ids.add(video_id)\n",
    "                file.write(\"\\n\".join([\n",
    "                    f'https://www.youtube.com/watch?v={yt_id}'\n",
    "                    for yt_id in list(unused_yt_ids)\n",
    "                    if yt_id\n",
    "                ]))\n",
    "    # Search Next Batch\n",
    "    print(\"\") # Just New Line for Better Output\n",
    "    print(f'...Searching [{last_yt_videos_len} Total Video]...')\n",
    "    time.sleep(1)\n",
    "    yt_list.next()\n",
    "    yt_videos = yt_list.result()[\"result\"]\n",
    "    next_yt_videos_len = len(yt_videos)\n",
    "    # Stop If No More Videos Found\n",
    "    if next_yt_videos_len > 0:\n",
    "        last_yt_videos_len += next_yt_videos_len\n",
    "        continue\n",
    "    clear_output(wait=True)\n",
    "    print(\"No More Videos Found\")\n",
    "    break"
   ]
